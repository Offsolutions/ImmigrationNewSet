@model IEnumerable<ImmigrationNewSetup.Models.Logs>
@{
    ViewBag.Title = "Logs";
    Layout = "~/Views/Shared/Staff.cshtml";
}

<h2>Logs</h2>

<div class="col-lg-6">
    <div class="card">
        <div class="card-header card-header-divider">Latest Activity<span class="card-subtitle">This is a custom timeline widget</span></div>
        <div class="card-body">
            <ul class="user-timeline">
@foreach (var item in Model)
{
    <li class="latest">
        <div class="user-timeline-date">@item.date</div>
        
        <div class="user-timeline-description">@item.message</div>
    </li>
}
              
            </ul>
        </div>
    </div>
</div>